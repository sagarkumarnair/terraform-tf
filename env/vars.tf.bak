variable "instance_type" {
  default = "t2.micro"
}

variable "sg_ports" {
  type        = list(number)
  description = "list of ingress ports"
  default     = [443, 22, 80]
}

variable "server_port" {
  default = "8080"
}

variable "key_name" {
  default = "varunp"
}

variable "aws_alb" {
  default = ""
}

variable "subnet_id" {
  default = ""
}
variable "name" {
  default = ""
}
